{
  "name": "peronsalSite",
  "version": "1.0.0",
  "description": "my personal website",
  "main": "index.js",
  "engines": {
    "node": ">= 7.0.0"
  },
  "scripts": {
    "dev": "node dev",
    "validate": "check-node-version --node '>= 7.0.0'",
    "prep": "npm run validate && npm run setup",
    "postinstall": "npm run prep",
    "build": "webpack",
    "build-watch": "npm run build -- -w",
    "build-dev": "cross-env NODE_ENV=development npm run build-watch",
    "start": "firebase serve",
    "test-watch": "npm run test -- --watch --reporter=min",
    "lint": "esw . --ignore-path .gitignore --ext '.js,.jsx'",
    "lint-watch": "npm run lint -- -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Meschreiber/peronsalSite.git"
  },
  "keywords": [
    "react"
  ],
  "author": "Maria Schreiber",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Meschreiber/personalSite/issues"
  },
  "homepage": "https://github.com/Meschreiber/personalSite#readme",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.4",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^3.3.0"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "chalk": "^2.0.1",
    "firebase": "^4.1.3",
    "google-fonts": "^1.0.0",
    "homedir": "^0.6.0",
    "jquery": "^3.2.1",
    "material-ui": "^0.18.6",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "react-tap-event-plugin": "^2.0.1",
    "webpack-livereload-plugin": "^0.11.0"
  }
}
